<%= form_for :item, url: items_path do |f| %>
    <p>
        <%= f.label :title %><br>
        <%= f.text_field :title %>
    </p>

    <p>
        <%= f.label :description %><br>
        <%= f.text_area :description %>
    </p>

    <p>
        <%= f.label :image %>
        <%= f.text_field :image %>
    </p>

    <p>
        <%= f.submit %>
    </p>
<% end %>
